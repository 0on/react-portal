{
  "name": "react-portal",
  "main": "build/portal",
  "description": "Simple React component for transportation of your modals, lightboxes, etc. to document.body",
  "author": "Vojtech Miksu <vojtech@miksu.cz>",
  "version": "1.5.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tajo/react-portal"
  },
  "scripts": {
    "build": "mkdir -p build && babel ./lib/portal.js --out-file ./build/portal.js",
    "build:examples": "babel ./examples/index.js --out-file ./examples/bundle.js",
    "test": "mocha --compilers js:babel-register",
    "lint": "eslint examples lib",
    "prepublish": "npm run build"
  },
  "keywords": [
    "react",
    "react-component",
    "modal",
    "lightbox",
    "react-portal",
    "portal",
    "transportation"
  ],
  "peerDependencies": {
    "react": "^0.14.0",
    "react-dom": "^0.14.0"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-eslint": "^4.1.6",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "enzyme": "^1.2.0",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.14.0",
    "jsdom": "^7.2.2",
    "mocha": "^2.3.4",
    "react-addons-test-utils": "^0.14.6",
    "sinon": "^1.17.2",
    "tween.js": "^16.3.1"
  }
}
